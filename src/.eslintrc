{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "arrow-parens": "off",
        "comma-dangle": "off",
        "require-jsdoc": "off",
        "no-unused-vars": "off",
        "no-invalid-this": "off"
    },
    "extends": ["eslint:recommended", "google"]
}